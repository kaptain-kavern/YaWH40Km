<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase" Name="AirdropBase" Abstract="True">
    <thingClass>FactionColors.Deepstriker_Incoming</thingClass>
    <altitudeLayer>Building</altitudeLayer>
    <pathCost>150</pathCost>
    <blockWind>true</blockWind>
    <passability>Impassable</passability>
    <fillPercent>1.0</fillPercent>
    <size>(6,3)</size>
    <statBases>
      <MaxHitPoints>1200</MaxHitPoints>
      <Flammability>0</Flammability>
      <Beauty>-100</Beauty>
    </statBases>
    <tickerType>Normal</tickerType>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <rotatable>true</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>true</neverMultiSelect>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <soundImpactDefault>BulletImpactMetal</soundImpactDefault>
    <drawerType>MapMeshOnly</drawerType>
    <repairEffect>ConstructMetal</repairEffect>
    <castEdgeShadows>false</castEdgeShadows>
    <staticSunShadowHeight>1.0</staticSunShadowHeight>
    <forceDebugSpawnable>true</forceDebugSpawnable>
    <building>
      <soundAmbient>CrashedShipPart_Ambience</soundAmbient>
      <roofCollapseDamageMultiplier>0.2</roofCollapseDamageMultiplier>
    </building>
    <BelongsToFactions>
      <li>Outlander</li>
      <li>Pirate</li>
      <li>SpacerHostile</li>
      <li>ImperialGuard</li>
      <li>AdeptusSororitas</li>
      <li>ChaosCult</li>
      <li>ImperialGuard</li>
    </BelongsToFactions>
  </ThingDef>

  <ThingDef ParentName="AirdropBase" Class="FactionColors.Deepstriker_ThingDef">
    <defName>Valkyrie_Incoming</defName>
    <label>Incoming Valkyrie</label>
    <RemainingDef>Valkyrie</RemainingDef>
    <description>An incoming Valkyrie VTOL assault carrier.</description>
    <thingClass>Building_PsychicEmanator</thingClass>
    <graphicData>
      <texPath>Things/IoM/Valkyrie/Valkyrie</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(6,6)</drawSize>
    </graphicData>
  </ThingDef>

  <ThingDef ParentName="AirdropBase" Class="FactionColors.Deepstriker_ThingDef">
    <defName>Valkyrie</defName>
    <label>Valkyrie</label>
    <LeavingDef>Valkyrie_Leaving</LeavingDef>
    <description>A Valkryie VTOL assault carrier.</description>
    <thingClass>Building_PsychicEmanator</thingClass>
    <graphicData>
      <texPath>Things/IoM/Valkyrie/Valkyrie</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(6,6)</drawSize>
    </graphicData>
  </ThingDef>

  <ThingDef ParentName="AirdropBase" Class="FactionColors.Deepstriker_ThingDef">
    <defName>Valkyrie_Leaving</defName>
    <label>Valkyrie</label>
    <description>A Valkryie VTOL assault carrier taking off.</description>
    <thingClass>Building_PsychicEmanator</thingClass>
    <graphicData>
      <texPath>Things/IoM/Valkyrie/Valkyrie</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(6,6)</drawSize>
    </graphicData>
  </ThingDef>


</Defs>
